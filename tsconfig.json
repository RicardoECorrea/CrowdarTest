{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["es6", "dom"],
      "types": ["cypress", "node"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    },
    "include": ["cypress/**/*.ts"]
  }
  